  // #region Editor
  "editor.lineNumbers"                        : "on",
  "editor.inlayHints.enabled"                 : "off",
  "editor.indentSize"                         : "tabSize",
  "editor.acceptSuggestionOnEnter"            : "off",
  "editor.wordWrap"                           : "off",
  "editor.renderWhitespace"                   : "trailing",
  "editor.renderControlCharacters"            : true,
  "editor.renderFinalNewline"                 : "on",
  "editor.renderLineHighlight"                : "line",
  "editor.fontLigatures"                      : true,
  "editor.trimAutoWhitespace"                 : true,
  "editor.acceptSuggestionOnCommitCharacter"  : false,
  "editor.mouseWheelZoom"                     : true,
  "editor.smoothScrolling"                    : true,
  "editor.cursorSmoothCaretAnimation"         : "on",
  "editor.roundedSelection"                   : true,
  "editor.glyphMargin"                        : true,
  "editor.snippetSuggestions"                 : "top",
  "editor.multiCursorModifier"                : "ctrlCmd",
  "editor.fontWeight"                         : "normal",
  "editor.cursorBlinking"                     : "smooth",
  "editor.cursorStyle"                        : "line",
  "editor.cursorWidth"                        : 4,
  "editor.tabSize"                            : 2,
  "editor.fontSize"                           : 18,
  "editor.inlayHints.fontSize"                : 10,
  "editor.codeLensFontSize"                   : 10,
  "editor.suggestFontSize"                    : 14,
  "editor.suggestLineHeight"                  : 14,
  "editor.suggest.showInlineDetails"          : true,
  "editor.inlineSuggest.enabled"              : false,
  "editor.lineHeight"                         : 21,
  "editor.letterSpacing"                      : 0,
  "editor.maxTokenizationLineLength"          : 500,
  "editor.cursorSurroundingLines"             : 10,
  "editor.rulers"                             : [80],
  "editor.suggest.preview"                    : false,
  "editor.insertSpaces"                       : true,
  "editor.scrollBeyondLastLine"               : true,
  "editor.scrollPredominantAxis"              : true,
  "editor.dragAndDrop"                        : false,
  "editor.hover.above": true,
  "editor.hover.enabled": true,
  "editor.hover.delay": 500,
  "editor.hover.hidingDelay": 0,
  "editor.hover.sticky": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.quickSuggestionsDelay": 10,
  "editor.guides.indentation"                 : false,
  "editor.guides.bracketPairs"                : false,
  "editor.guides.bracketPairsHorizontal"      : "active",
  "editor.guides.highlightActiveBracketPair"  : true,
  "editor.guides.highlightActiveIndentation"  : "always",
  "editor.minimap.enabled"          : true,
  "editor.minimap.autohide"         : true,
  "editor.minimap.renderCharacters" : false,
  "editor.minimap.showSlider"       : "mouseover",
  "editor.minimap.maxColumn"        : 100,
  "editor.minimap.scale"            : 1,
  "editor.minimap.side"             : "right",
  "editor.minimap.size"             : "proportional",
  // #endregion

  // #region etc
  "update.mode"             : "none",
  "update.showReleaseNotes" : true,
  "diffEditor.wordWrap"                       : "off",
  "scm.inputFontFamily"                       : "'editor'",
  "insert-unicode.enableRecentlyUsed"         : false,
  "open-in-browser.default"                   : "firefox",
  "color-highlight.markerType"                : "underline",
  "livePreview.notifyOnOpenLooseFile"         : false,
  "terminal.integrated.fontSize"              : 16,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.enableMultiLinePasteWarning": "auto",
  "files.autoSave": "off",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "debug.console.fontSize"                    : 16,
  "explorer.sortOrder": "default",
  "breadcrumbs.enabled": true,
  // #endregion

  // #region Extensions
  "extensions.autoUpdate": false,

  // #region betterSnippets
  "betterSnippets.typingSnippets": [
    {
        "sequence": "cb ",
        "body":"($1) => $2"
    }
  ],
  // #endregion

  // #region liveServer
  "liveServer.settings.CustomBrowser" : "firefox",
  "liveServer.settings.donotShowInfoMsg": true,
  // #endregion

  // #region better-comments
  "better-comments.tags": [
    //!Hello
    //?Hello
    //*Hello
    //$Hello
    //&Hello
    //%Hello
    //~Hello
    //=Hello
    //+Hello
    //-Hello
