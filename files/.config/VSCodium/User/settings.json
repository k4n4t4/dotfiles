  // #region trailing-spaces
  "trailing-spaces.logLevel": "warn",
  "trailing-spaces.backgroundColor": "rgba(255,0,0,0.3)",
  "trailing-spaces.borderColor": "rgba(255,100,100,0.15)",
  "trailing-spaces.trimOnSave": true,
  "trailing-spaces.highlightCurrentLine": false,
  "trailing-spaces.includeEmptyLines": true,
  // #endregion

  // #region commands
  "commands.commands": {
    "Folder": {
      "nestedItems": {
        "openSettingJSON": {
          "command": "workbench.action.openApplicationSettingsJson",
          "icon": "settings",
          "statusBar": {
            "alignment": "left",
            "text": "",
            "priority": 20000
          }
        },
        "Toggle StatusBar Visible": {
          "command": "commands.toggleSetting",
          "icon": "layout-statusbar",
          "args": {
            "setting": "workbench.statusBar.visible",
            "value": [
              true,
              false
            ]
          }
        },
        "Toggle Breadcrumb Visible": {
          "command": "commands.toggleSetting",
          "icon": "note",
          "args": {
            "setting": "breadcrumbs.enabled",
            "value": [
              true,
              false
            ]
          }
        },
        "Toggle GlyphMargin Visible": {
          "command": "commands.toggleSetting",
          "icon": "debug-continue",
          "args": {
            "setting": "editor.glyphMargin",
            "value": [
              true,
              false
            ]
          }
        },
        "Toggle LineNumbers": {
          "command": "commands.toggleSetting",
          "icon": "gripper",
          "args": {
            "setting": "editor.lineNumbers",
            "value": [
              "on",
              "relative",
            ]
          }
        },
        "openTerminal": {
          "command": "workbench.action.terminal.toggleTerminal",
          "icon": "terminal",
        }
      }
    }
  },
  // #endregion

  // #region bracket-pair-colorizer-2
  "bracket-pair-colorizer-2.colors": [
    "#AAAAAA",
    "#888888",
    "#666666",
  ],
  "bracket-pair-colorizer-2.unmatchedScopeColor"     : "gray",
  "bracket-pair-colorizer-2.colorMode"               : "Independent",
  "bracket-pair-colorizer-2.highlightActiveScope"    : true,
  "bracket-pair-colorizer-2.showHorizontalScopeLine" : true,
  "bracket-pair-colorizer-2.showBracketsInGutter"    : false,
  "bracket-pair-colorizer-2.showBracketsInRuler"     : true,
  "bracket-pair-colorizer-2.showVerticalScopeLine"   : true,
  // #endregion

  // #region BracketHighlighter
  "BracketHighlighter.enableExtension": true,
  "BracketHighlighter.border": "",
  "BracketHighlighter.blurOutOfScopeText": false,
  "BracketHighlighter.blurOpacity": "0.8",
  "BracketHighlighter.textColor": "#CECECE",
  "BracketHighlighter.ignoreContent": true,
  "BracketHighlighter.customSymbols": [
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "{",
      "close": "}"
    },
  ],
  // #endregion

  // #region indentRainbow
  "indentRainbow.colors": [
    "#88888805",
    "#55555505"
  ],
  "indentRainbow.errorColor": "rgba(44,44,44,0.2)",
  // #endregion

  // #region vscodeWorkspaceSwitcher
  "vscodeWorkspaceSwitcher.showTreeView": false,
  "vscodeWorkspaceSwitcher.paths": [
    "~/Documents/VSCode/workspaces"
  ],
  // #endregion

  // #region git
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "never",
  "github.gitProtocol": "https",
  // #endregion

  // #region colorIdentifiersMode
  "colorIdentifiersMode.ignoredLanguages": [],
  "colorIdentifiersMode.manualColors": [
    "#EA9090",
    "#EAAA90",
    "#EAEA90",
    "#AAEA90",
    "#90EA90",
    "#90EAAA",
    "#90EAEA",
    "#90AAEA",
    "#9090EA",
    "#AA90EA",
    "#EA90EA",
    "#EA90AA"
  ],
  "colorIdentifiersMode.method": "sequential",
  "colorIdentifiersMode.paletteMode": "manual",
  "colorIdentifiersMode.tokenKinds": [
    "variable",
    "parameter",
    "property",
    "method"
  ],
  // #endregion

  // #region cSpell
  "cSpell.enableFiletypes": [],
  "cSpell.userWords": [
    "codepaths",
    "colours",
    "Stylesheet",
    "tsbuildinfo"
  ],
  "cSpell.ignoreWords": [
    "rgba",
    "endregion",
    "autofetch",
    "donot",
    "autohide"
  ],
  "cSpell.diagnosticLevel": "Hint",
  // #endregion

  // #region errorLens
  "errorLens.decorations": {
    "errorRange": {},
    "warningRange": {},
    "infoRange": {},
    "hintRange": {}
  },
  "errorLens.alignMessage": {
    "start": 0,
    "end": 0,
    "minimumMargin": 0
  },
  "errorLens.fontStyleItalic": true,
  "errorLens.margin": "0.5ch",
  "errorLens.gutterIconsEnabled": true,
  "workbench.productIconTheme": "icons-carbon",
  "workbench.colorTheme": "Monokai Dimmed"
  // #endregion

  // #endregion Extensions

}
