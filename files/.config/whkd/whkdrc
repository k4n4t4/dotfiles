.shell powershell

# oem_1 is ;
# oem_3 is @
# oem_4 is [
# oem_6 is ]

alt + r                 : taskkill /f /im whkd.exe; Start-Process whkd -WindowStyle hidden

alt + b                 : start firefox
alt + p                 : start firefox --private-window

alt + t                 : wt
alt + shift + t         : Start-Process -FilePath "wt" -Verb RunAs
alt + c                 : wt -p "PowerShell"
alt + shift + c         : Start-Process -FilePath "wt" -Verb RunAs -ArgumentList "-p", "PowerShell"
