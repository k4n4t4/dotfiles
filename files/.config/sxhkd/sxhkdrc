super + t
  alacritty

super + e
  dolphin

super + b
  firefox

super + v
  code || codium

super + r
  rofi -show drun

# AudioVolume
XF86AudioRaiseVolume
    pactl set-sink-volume 0 +3%
XF86AudioLowerVolume
    pactl set-sink-volume 0 -3%
XF86AudioMute
    pactl set-sink-mute 0 toggle

super + Escape
  pkill -USR1 -x sxhkd

super + button1 + drag
  bspc node -t floating 

super + button3
  bspc node -t ~floating

alt + F4
  bspc node -k

alt + Tab
  bspc node -f last

super + c
  bspc node -c

super + shift + r
  bspc wm -r

super + shift + q
  bspc quit

super + F11
  bspc node -t ~fullscreen
